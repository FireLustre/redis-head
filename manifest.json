{
    "manifest_version": 2,
    "name": "Redis Head",
    "description": "Redis Manager.",
    "version": "0.4",
    "icons": {
        "16": "images/redis-head-16.png",
        "128": "images/redis-head-128.png"
    },
    "app": {
        "background": {
            "scripts": [
                "./js/background.js"
            ]
        }
    },
    "sockets": {
        "tcp": {
            "connect": "*"
        }
    },
    "permissions": [
        "storage",
        "http://*/*",
        "https://*/*"
    ]
}