{
    "manifest_version": 2,
    "name": "Redis Head",
    "description": "Redis Manager.",
    "version": "0.3",
    "icons": {
        "16": "images/redis-head-16.png",
        "128": "images/redis-head-128.png"
    },
    "app": {
        "background": {
            "scripts": [
                "./js/tcp.js",
                "./js/background.js"
            ]
        }
    },
    "sockets": {
        "tcp": {
            "connect": "*"
        },
        "tcpServer": {
            "listen": ":1117"
        }
    },
    "permissions": [
        "storage",
        "http://*/*",
        "https://*/*"
    ]
}